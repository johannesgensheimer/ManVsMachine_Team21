# Slack Bot Configuration
# Copy this file to .env and fill in your values

# Bot User OAuth Token (starts with xoxb-)
SLACK_BOT_TOKEN=xoxb-your-bot-token-here

# App-Level Token for Socket Mode (starts with xapp-)
SLACK_APP_TOKEN=xapp-your-app-token-here

# Signing Secret from Slack App Settings
SLACK_SIGNING_SECRET=your-signing-secret-here

# Backend API Configuration
BACKEND_API_URL=http://localhost:3000

# Server Ports
PORT=3000
SLACK_PORT=3001

# Optional: Channel to send startup notifications
SLACK_NOTIFICATION_CHANNEL=

# Database Configuration (for backend)
DATABASE_URL="postgresql://publicuser:Hackathon123!@YOUR_CLOUD_SQL_IP:5432/hackathon"

# OpenAI Configuration (for backend)
OPENAI_API_KEY=your-openai-api-key-here

# Optional: Debugging
DEBUG=true