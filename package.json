{
  "name": "manvsmachine-team21",
  "version": "1.0.0",
  "description": "LangChain database integration for supplier management with Slack bot interface",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "tsc",
    "server": "ts-node src/server.ts",
    "server:watch": "nodemon --exec ts-node src/server.ts",
    "slack": "node server.js",
    "slack:dev": "nodemon server.js",
    "dev:ts": "ts-node src/index.ts",
    "dev:ts:watch": "nodemon --exec ts-node src/index.ts",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:slack": "node test-slack-integration.js"
  },
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "@slack/bolt": "^4.4.0",
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^4.18.2",
    "openai": "^4.104.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^20.10.5",
    "nodemon": "^3.1.10",
    "prisma": "^5.7.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "langchain",
    "database",
    "supplier-management",
    "postgresql",
    "slack-bot"
  ],
  "author": "Team21",
  "license": "MIT"
}
