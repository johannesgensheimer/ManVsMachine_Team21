{
  "name": "manvsmachine-team21",
  "version": "1.0.0",
  "description": "LangChain database integration for supplier management",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "server": "ts-node src/server.ts",
    "server:watch": "nodemon --exec ts-node src/server.ts",
    "dev": "ts-node src/index.ts",
    "dev:watch": "nodemon --exec ts-node src/index.ts",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio"
  },
  "dependencies": {
    "@langchain/core": "^0.1.52",
    "@langchain/community": "^0.0.43",
    "@langchain/openai": "^0.0.25",
    "langchain": "^0.1.25",
    "@prisma/client": "^5.7.1",
    "dotenv": "^16.3.1",
    "zod": "^3.22.4",
    "express": "^4.18.2",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "axios": "^1.6.2",
    "nodemon": "^3.0.2",
    "prisma": "^5.7.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "keywords": ["langchain", "database", "supplier-management", "postgresql"],
  "author": "Team21",
